<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="gmail.yuchisong7.portfolio.domain">
	<class name="SpotReview" table="spotreview">
		<id name="commentid" column="commentid" />
		<property name="nickname" column="nickname" />
		<property name="spotid" column="spotid" />
		<property name="score" column="score" />
		<property name="commentdate" column="commentdate" />
		<property name="message" column="message" />
	
	</class>
</hibernate-mapping>
